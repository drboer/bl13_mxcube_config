<object class="XalocMultiCollect">	

  <object href="/mxlocal" role="beamline_configuration"/>

  <object href="/cats" role="sample_changer"/>	
  <object href="/lims-client-mockup" role="dbserver"/>
  <object href="/minidiff" role="diffractometer"/>
  <object href="/mach" role="machine_current"/>

  <object href="/safshut" role="safety_shutter"/>	
  <object href="/energyscan" role="energy"/>
  <object href="/cryospy" role="cryo_stream"/>
  <object href="/feshut" role="undulators"/>
  <object href="/transmission-mockup" role="transmission"/>
  <object href="/resolution-mockup" role="resolution"/>
  <object href="/eh1/DtoX" role="detector_distance"/>
  <object href="/flux" role="flux"/>

  <doorname>door/mxcube/1</doorname>

  <command type="sardana" name="get_resolution_at_corner">getMxResolutionAtCorner</command>
  <command type="sardana" name="get_beam_size_x">getMxBeamSizeX</command>
  <command type="sardana" name="get_beam_size_y">getMxBeamSizeY</command>
  <command type="sardana" name="get_slit_gap_h">getMxSlitGapHorizontal</command>
  <command type="sardana" name="get_slit_gap_v">getMxSlitGapVertical</command>
  <command type="sardana" name="get_beam_shape">getMxBeamShape</command>
  <command type="sardana" name="get_beam_centre_x">getMxBeamCentreX</command>
  <command type="sardana" name="get_beam_centre_y">getMxBeamCentreY</command>
  <command type="sardana" name="close_fast_shutter">msclose</command>
  <command type="sardana" name="open_fast_shutter">msopen</command>
  <command type="sardana" name="adjust_gains">adjust_i0_i1_gain</command>
  <command type="sardana" name="prepare_oscillation">osc_prepare</command>
  <command type="sardana" name="do_oscillation">_oscillation</command>
  <command type="sardana" name="build_collect_seq">build_collect_seq</command>
  <command type="sardana" name="prepare_acquisition">pilatus_prep</command>
  <command type="sardana" name="start_acquisition">ccdstart</command>
  <command type="sardana" name="detector_readout">ccdread</command>
  <command type="sardana" name="write_image">ccdsave</command>
  <command type="sardana" name="flush_detector">ccdflush</command>
  <command type="sardana" name="reset_detector">pilatus_cleanup</command>
  <command type="sardana" name="prepare_musst">musstPX_loadprog</command>
  <command type="sardana" name="prepare_beamline">prodc_prepare_beamline</command>
  <command type="sardana" name="specific_collect_frame_hook">pilatus_postframe_actions</command>
  <command type="sardana" name="local_set_experiment_type">pilatus_set_experiment_type</command>

  <channel type="sardana" taurusname="dev/not/set" name="parameters">datacollection_parameters</channel>
  <channel type="sardana" taurusname="dev/not/set" name="image_intensity">CURRENT_IMAGE_INTENSITY</channel>
  <channel type="sardana" taurusname="dev/not/set" name="take_dark">MXCOLLECT_PARS/TakeDarkFlag</channel>
  <channel type="sardana" taurusname="dev/not/set" name="spec_messages">eprodc_log_message</channel>
  <channel type="sardana" taurusname="dev/not/set" name="shutterless">PILATUS_SHUT</channel>
  <channel type="sardana" taurusname="dev/not/set" name="helical">HELICAL_OSCIL</channel>
  <channel type="sardana" taurusname="dev/not/set" name="helical_pos">HELICAL_OSCIL_POS</channel>
  <channel type="sardana" taurusname="dev/not/set" name="helical_pars">HELICAL_PARS</channel>

  <directory_prefix>mxcube_test</directory_prefix>
  
  <input_files_server>localhost:5698</input_files_server>

  <auto_processing>
    <program>
      <executable>/opt/pxsoft/bin/autoproc_master.pl</executable>
      <event>before image after end_multicollect</event>
    </program>
    <program>
      <executable>/users/blissadm/bin/autoproc-launcher.py</executable>
      <event>before</event>
    </program> 
  </auto_processing>
</object>
