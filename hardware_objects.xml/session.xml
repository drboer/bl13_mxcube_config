<object class = "ALBASession" role = "session">  
  <!-- ATTENTION . base data directories are overwritten in ALBA -->
  <!--   the value in ldap homeDirectory is used for the three base directories -->
  <!--      base_directory, base_process_directory, base_archive_directory -->
  <!--   check the code in ALBA/ALBAISPyBClient.py -->
  <synchrotron_name>ALBA</synchrotron_name>	
  <endstation_name>MX-BL13</endstation_name>
  <beamline_name>MX-BL13</beamline_name>
  <file_info>
    <file_suffix>cbf</file_suffix>
    <precision>'05'</precision>    
    <!-- base_directory>/beamlines/bl13/projects/cycle2017-I</base_directory -->
    <base_directory>/beamlines/bl13/projects/cycle2017/ispybtest</base_directory>
    <raw_data_folder_name>RAW_DATA</raw_data_folder_name>
    <!-- processed_data_base_directory>/beamlines/bl13/projects/cycle2017-I</processed_data_base_directory -->
    <processed_data_base_directory>/beamlines/bl13/projects/cycle2017/ispybtest</processed_data_base_directory>
    <processed_data_folder_name>PROCESSED_DATA</processed_data_folder_name>
    <!-- archive_base_directory>/beamlines/bl13/projects/cycle2017-I/ispyb</archive_base_directory -->
    <archive_base_directory>/beamlines/bl13/projects/cycle2017/ispybtest</archive_base_directory>
    <archive_folder>archive</archive_folder>
  </file_info>
</object>
